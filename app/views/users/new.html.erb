<div class="page-header">
  <h1>Sign Up</h1>
</div>

<%= form_for :user, url: users_path do |f| %>
  <div>
    <%= f.label :email  %>
    <%= f.email_field :email  %>
  </div>

  <div>
    <%= f.label :password  %>
    <%= f.password_field :password  %>
  </div>

  <div>
    <%= f.label :password_confirmation  %>
    <%= f.password_field :password_confirmation  %>
  </div>

  <div>
    <%= f.submit "Sign Up"  %>
  </div>
<% end %>
